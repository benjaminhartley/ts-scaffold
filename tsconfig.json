        "module": "commonjs",
        "lib": ["es2019"],
        "target": "es2019",
        "strict": true,
        "esModuleInterop": true,
        "noImplicitAny": false,
        "sourceMap": true,
        "outDir": "dist",
        "resolveJsonModule": true
